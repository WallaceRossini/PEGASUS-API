{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "quotes":["error","single"],
      "space-before-blocks":"error",
      "camelcase":"error",
      "curly":"error",
      "handle-callback-err":"off",
      "space-in-parens":"error"
    }
}
